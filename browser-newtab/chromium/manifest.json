{
	"manifest_version": 3,
	"name": "Homepage",
	"description": "customized homepage",
	"version": "1.0",

	"icons": {
        	"16": "icons/chrome_icon-48.png",
        	"32": "icons/chrome_icon-48.png",
        	"48": "icons/chrome_icon-48.png",
        	"96": "icons/chrome_icon-240.png",
